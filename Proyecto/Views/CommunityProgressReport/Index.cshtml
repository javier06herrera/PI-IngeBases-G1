@model Proyecto.Models.CommunityProgressReportModel

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <script src="~/Scripts/js/CommunityProgressReport.js"></script>
    <script src="~/Scripts/js/Chart.js"></script>
    <script type="text/javascript" src="~/Scripts/js/jquery-3.4.1.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js" integrity="sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4" crossorigin="anonymous"></script>
    <script type="text/javascript" src="~/Scripts/js/bootstrap.min.js"></script>
    <script src="~/Scripts/js/bootstrap-multiselect.js"></script>
    <link rel="stylesheet" type="text/css" href="~/Content/css/bootstrap-multiselect.css">
    <link rel="stylesheet" href="~/Content/css/bootstrap.min.css" type="text/css" />
</head>


<body>
    @using (Html.BeginForm())
    {

        <div class="container-fluid">
            <label>Member rank</label>
            <select id="memberCategories" multiple="multiple">
                <option value="Cordinator">Cordinator</option>
                <option value="Core">Core</option>
                <option value="Active">Active</option>
                <option value="Peripheral">Peripheral</option>
                <option value="External">External</option>
                <option value="Educator">Educator</option>
            </select>

            <div>
                <label>Filters</label>
                <select id="filters" multiple="multiple">
                    <option value="Number of articles">Number of articles</option>
                    <option value="Article score">Article score</option>
                    <option value="Number of articles peer category and topic">Number of articles peer category and topic</option>
                    <option value="Access Count peer category and topic">Access Count peer category and topic</option>
                </select>
            </div>
            <button onclick="generateReport()" class="btn btn-primary">Generate</button>
        </div>

        <script type="text/javascript">
            $(document).ready(function () {
                $('select').multiselect(
                    {
                        includeSelectAllOption: true,
                        numberDisplayed: 5
                    });
            });
        </script>

        
    }
</body>

</html>